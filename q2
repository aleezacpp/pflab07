
#include <stdio.h>
int main(){
    int x; //array size
    int i;
    printf("enter array size: \n");
    scanf("%d", &x);
    int arr[x];
    for (i=0; i<x; i++){
        printf("arr[%d] : \n", i);
        scanf("%d", &arr[i]);
    }
    for (i=(x-1);i>=0;i--){
        printf("%d",arr[i]);
    }
    return 0;
}
